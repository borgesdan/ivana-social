<Page x:Class="Ivana.Pages.Print.RegistryPrintPage2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Ivana.Pages.Print"
      mc:Ignorable="d" 
      d:DesignHeight="1222" d:DesignWidth="793"
      Title="RegistryPrintPage2">

    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../../Dictionaries/Default.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>

    <Grid Background="White" x:Name="MainGrid">
        <StackPanel  x:Name="MainStack" Width="700">
            <TextBlock HorizontalAlignment="Center" Margin="0">
                <Run FontWeight="Bold" FontSize="{StaticResource MediumSize}">COMPOSIÇÃO FAMILIAR</Run>
            </TextBlock>
            <StackPanel x:Name="RelationshipStack">
            </StackPanel>            
            <TextBlock HorizontalAlignment="Center" Margin="0 20 0 0">
                <Run FontWeight="Bold" FontSize="{StaticResource MediumSize}">SITUAÇÃO SÓCIOECONÔMICA</Run>
            </TextBlock>
            <StackPanel x:Name="EconomicStack">
            </StackPanel>
        </StackPanel>
    </Grid>
</Page>
